<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@drawable/layouthome"
    tools:context=".admin.HomeAdminActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Selamat Datang di Menu Utama"
        android:textColor="#000"
        android:textSize="25sp"
        android:layout_gravity="center_horizontal" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonRL"
        android:gravity="center"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="300dp">

        <Button
            android:id="@+id/menu_panggilantri"
            android:layout_width="130dp"
            android:layout_height="160dp"
            android:background="@drawable/pannggil_antrian"
            />

        <Button
            android:id="@+id/menu_daftarklinik"
            android:layout_width="130dp"
            android:layout_height="160dp"
            android:background="@drawable/daftarknilik"
            android:layout_toRightOf="@+id/menu_panggilantri"
            />

        <Button
            android:id="@+id/menu_informasi"
            android:layout_width="130dp"
            android:layout_height="160dp"
            android:gravity="right"
            android:background="@drawable/informasi"
            android:layout_toRightOf="@+id/menu_daftarklinik"
            />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:layout_weight="1"
        android:gravity="center|bottom"
        android:orientation="vertical" >

        <Button
            android:id="@+id/bt_Logoutadmin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center"
            android:background="@drawable/keluarbuuton"/>

    </LinearLayout>

</LinearLayout>